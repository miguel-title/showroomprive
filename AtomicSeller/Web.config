      <error statusCode="500" redirect="/Resources/errorPages/500.aspx"/>
    </customErrors>
  </system.web>
  <system.webServer>
    <httpErrors errorMode="DetailedLocalOnly">
      <remove statusCode="404"/>
      <error statusCode="404" path="/Resources/errorPages/404.html" responseMode="ExecuteURL"/>
      <remove statusCode="500"/>
      <error statusCode="500" path="/Resources/errorPages/500.html" responseMode="ExecuteURL"/>
    </httpErrors>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
